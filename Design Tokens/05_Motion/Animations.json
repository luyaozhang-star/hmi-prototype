{
  "motion": {
    "duration": {
      "fast": {
        "value": 150,
        "type": "number"
      },
      "standard": {
        "value": 300,
        "type": "number"
      },
      "slow": {
        "value": 500,
        "type": "number"
      }
    },
    "easing": {
      "default": {
        "value": "cubic-bezier(0.25, 0.46, 0.45, 0.94)",
        "type": "cubic-bezier"
      },
      "entrance": {
        "value": "cubic-bezier(0.34, 1.56, 0.64, 1)",
        "type": "cubic-bezier"
      },
      "exit": {
        "value": "cubic-bezier(0.66, 0, 0.66, 0.07)",
        "type": "cubic-bezier"
      },
      "smooth": {
        "value": "cubic-bezier(0.4, 0, 0.2, 1)",
        "type": "cubic-bezier"
      },
      "sharp": {
        "value": "cubic-bezier(0.4, 0, 0.6, 1)",
        "type": "cubic-bezier"
      }
    },
    "transition": {
      "fast-smooth": {
        "value": "{motion.duration.fast} {motion.easing.smooth}",
        "type": "transition"
      },
      "standard-smooth": {
        "value": "{motion.duration.standard} {motion.easing.default}",
        "type": "transition"
      },
      "slow-smooth": {
        "value": "{motion.duration.slow} {motion.easing.default}",
        "type": "transition"
      },
      "entrance-emphasis": {
        "value": "{motion.duration.standard} {motion.easing.entrance}",
        "type": "transition"
      },
      "exit-emphasis": {
        "value": "{motion.duration.fast} {motion.easing.exit}",
        "type": "transition"
      }
    }
  }
}
